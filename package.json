{
  "name": "alert-manager",
  "version": "1.0.0",
  "description": "Alert Manager App Plugin",
  "scripts": {
    "git-info": "echo \"{\\\"commit\\\": \\\"$(git log -1 --oneline)\\\"}\" > src/gitInfo.json",
    "build": "yarn git-info && grafana-toolkit plugin:build --coverage",
    "test": "grafana-toolkit plugin:test",
    "test:coverage": "grafana-toolkit plugin:test --coverage",
    "test:watch": "grafana-toolkit plugin:test --watch",
    "dev": "yarn git-info && grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:prettier": "prettier --loglevel=warn --check .",
    "lint:fix": "prettier --loglevel=warn --write . && yarn run lint --fix"
  },
  "author": "ALMGR",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@grafana/data": "7.5.10",
    "@grafana/toolkit": "7.5.10",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/codemirror": "^5.60.5",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.2.6",
    "babel-plugin-styled-components": "^2.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "react": "17.0.1"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "dependencies": {
    "@alrt/eslint-plugin-samsa": "0.1.2",
    "@alrt/ui": "0.0.29",
    "@ant-design/colors": "6.0.0",
    "@grafana/runtime": "11.2.3",
    "@grafana/ui": "11.2.3",
    "@pvm-ui/pvm-ui-components": "^1.0.18",
    "@types/react-router-dom": "5.1.8",
    "@types/styled-components": "5.1.12",
    "antd": "4.24.12",
    "codemirror": "^5.65.2",
    "file-saver": "^2.0.5",
    "immutability-helper": "3.1.1",
    "mobx": "6.3.2",
    "mobx-react-lite": "3.2.0",
    "moment-timezone": "0.5.34",
    "nanoid": "3.3.8",
    "react-codemirror2": "^7.2.1",
    "react-dnd": "14.0.2",
    "react-dnd-html5-backend": "14.0.0",
    "react-icons": "^4.10.1",
    "react-json-view": "^1.21.3",
    "react-router-dom": "5.2.0",
    "styled-components": "5.3.0",
    "tslib": "^2.3.1"
  },
  "resolutions": {
    "tinycolor2": "1.4.2",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "uplot": "1.6.31",
    "moment": "2.29.4",
    "@babel/runtime": "7.26.10",
    "xss": "1.0.10",
    "d3-color": "3.1.0",
    "marked": "4.0.10",
    "@babel/traverse": "7.23.2",
    "cross-spawn": "6.0.6",
    "@babel/runtime-corejs3": "7.26.10",
    "path-to-regexp": "1.9.0",
    "@braintree/sanitize-url": "6.0.1"
  }
}
